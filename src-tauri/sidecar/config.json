{
  "api": {
    "baseUrl": "http://192.168.0.20:9080/api",
    "endpoints": {
      "seats": "/discussion/seats",
      "speakers": "/discussion/speakers",
      "requests": "/discussion/requests",
      "updateSeat": "/discussion/seats",
      "settings": "/discussion/settings"
    }
  },
  "auth": {
